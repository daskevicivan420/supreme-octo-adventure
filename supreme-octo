        var newState = state
        newState.collage = elementsTransformer.rotate(
            gestureState,
            in: state.collage
        )
        
        return newState
    }
var newCollage = collage
        
        newCollage.stickers[stickerIndex] = stickerReducer.reduce(
            newCollage.stickers[stickerIndex],
            action
        )
